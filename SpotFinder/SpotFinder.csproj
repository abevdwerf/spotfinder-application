<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
    <BaseOutputPath>C:\Users\abeva\source\repos\abevdwerf\spotfinder-application\SpotFinder\bin\Debug</BaseOutputPath>
    <StartupObject>SpotFinder.App</StartupObject>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Pages\working-person-illustration.svg" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Expression.Drawing" Version="3.0.0" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
